{
  "name": "@ezcater/icons",
  "version": "1.0.0",
  "description": "ezCater icons available as React components",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Craig Cavalier",
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16"
  },
  "scripts": {
    "build": "run-s build:icons build:library",
    "build:icons": "svgr -d compiled-icons src",
    "build:library": "tsup"
  },
  "tsup": {
    "entry": ["compiled-icons"],
    "sourcemap": "inline",
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "legacyOutput": true
  }
}
